page{
  background-color: #dedede;
  padding: 20rpx;
  box-sizing:border-box
}
.search_row{
  height: 60rpx;
  display: flex;
  
}
.search_row input{
  flex: 1;
  height: 100%;
  background-color: #ffffff;
  padding-left: 20rpx;
  margin-right: 10rpx;
}
.search_row button{
  width: 110rpx;
  height: 100%;
  font-size: 26rpx;
  padding: 0;
  /* margin: 0 10rpx; */
  display: flex;
  justify-content: center;
  align-items: center;
}
.search_content{
  margin-top: 30rpx;
  /* padding-top: 30rpx; */
}
.search_content .search_item{
  background-color: #fff;
  font-size: 26rpx;
  padding: 15rpx 10rpx;
  border-bottom: 1rpx solid #ccc;
  overflow: hidden;
  white-space: nowrap;
  text-overflow: ellipsis;
}