page{
  background-color: #eeeeee;
}
.fb_main{
  padding: 20rpx;
  color: #666;
}
.fb_main .fb_title{
  
}
.fb_main .fb_tips{
  display: flex;
  flex-wrap: wrap;
}
.fb_main .fb_tips text{
  width: 30%;
  padding: 10rpx;
  text-align: center;
  background-color: #fff;
  margin: 20rpx 10rpx;
  box-sizing: border-box;
}
.fb_main .fb_content{
  background-color: #fff;
  margin-top: 20rpx;
}
.fb_main .fb_content textarea{
  padding: 10rpx;
}
.fb_main .fb_content .fb_tool{
  display: flex;
  flex-wrap: wrap;
  padding-bottom: 17rpx;
}
.fb_main .fb_content .fb_tool .up_ima_item{
  margin-left: 20rpx;
  margin-top: 20rpx;
}
.fb_main .fb_content .fb_tool button{
  margin: 0;
  width: 90rpx;
  height: 90rpx;
  font-size: 60rpx;
  padding: 0;
  display: flex;
  justify-content: center;
  align-items: center;
  margin-left: 20rpx;
  margin-top: 20rpx;
  color: #ccc;

}
.form_btn_wrap{
  margin-top: 15rpx;
  display: flex;
  /* 左对齐 */
  justify-content: flex-end;
  /* background-color: black; */
  padding-right: 20rpx;
}
.form_btn_wrap button{
  width: 30%;
  margin: 0;
  /* box-sizing: border-box; */

}